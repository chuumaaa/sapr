<%= form_for(test) do |f| %>
  <%= render 'shared/error_messages', object: f.object%>

  <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
    <%= f.label :name, "Name", class: "mdl-textfield__label"%>
    <%= f.text_field :name, class: "mdl-textfield__input" %>
  </div>

  <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
    <%= f.label :path, "Path", class: "mdl-textfield__label" %>
    <%= f.text_field :path, class: "mdl-textfield__input" %>
  </div>

  <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
    <%= f.label :description, "Description", class: "mdl-textfield__label" %>
    <%= f.text_field :description, class: "mdl-textfield__input" %>
  </div>
  
    <%= f.submit 'Start!', class: "mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored" %>
  
<% end %>
